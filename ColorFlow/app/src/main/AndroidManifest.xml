<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.colorflow">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Levels.LevelChooser" />
        <activity android:name=".Activities.Ingame.GameActivity" />
        <activity android:name=".Activities.Pregame.FlowModeSelectActivity" />
        <activity android:name=".Activities.Pregame.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.Ingame.ShowExpectedColorActivity" />
        <activity android:name=".Activities.Ingame.SpeedModeTimeOver" />
        <activity android:name=".Activities.Ingame.NewGameActivity"></activity>
    </application>

</manifest>